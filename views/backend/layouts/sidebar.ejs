<% if(typeof site_info.me !== 'undefined') { %>
     <% if(site_info.me.role == '1') { 
        var editor = 1 
    } else if(site_info.me.role == '2') { 
        var administrator = 1 
     } %>
<% } %>




<div id="wrap_sidebar">
    <div class="brand">
        <a href="<%= get_admin_url(); %>/dashboard" class="text-center">Administrator</a>
    </div>
    <ul class="list-group">
        <li>
            <a href="<%= get_admin_url(); %>/dashboard" class="list-group-item main  <% if(typeof site_info !== 'undefined' && site_info.page_slug=='dashboard') { %> active <% } %>">
                <i class="fa fa-tachometer" aria-hidden="true"></i>&nbsp; Dashboard
            </a>
        </li>
    </ul>
    <ul class="list-group">
        <li>
            <a href="<%= get_admin_url(); %>/articles" class="list-group-item main <% if(typeof site_info !== 'undefined' && (site_info.page_slug=='articles' || site_info.page_slug=='update_articles') ) { %> active <% } %>">
                <i class="fa fa-newspaper-o" aria-hidden="true"></i>&nbsp; Articles
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/articles" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='articles') { %> active <% } %>">
                All articles
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/articles/create" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='create_articles') { %> active <% } %>">
                Add new
            </a>
        </li>
        <% if(editor || administrator) { %>
            <li>
                    <a href="<%= get_admin_url(); %>/terms/categories" class="list-group-item  <% if(typeof site_info !== 'undefined' && (site_info.page_slug=='categories' || site_info.page_slug=='update_categories')) { %> active <% } %>">
                        Categories
                    </a>
                </li>
                <li>
                    <a href="<%= get_admin_url(); %>/terms/tags" class="list-group-item  <% if(typeof site_info !== 'undefined' && (site_info.page_slug=='tags' || site_info.page_slug=='update_tags')) { %> active <% } %>">
                        Tags
                    </a>
                </li>
        <% } %>
        
    </ul>
    <% if(administrator) { %>
    <ul class="list-group">
        <li>
            <a href="<%= get_admin_url(); %>/pages" class="list-group-item main  <% if(typeof site_info !== 'undefined' && (site_info.page_slug=='pages' || site_info.page_slug=='update_pages')) { %> active <% } %>">
                <i class="fa fa-file-text" aria-hidden="true"></i>&nbsp; Pages
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/pages" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='pages') { %> active <% } %>">
                All pages
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/pages/create" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='create_pages') { %> active <% } %>">
                Add new
            </a>
        </li>
    </ul>
    <ul class="list-group">
        <li>
            <a href="<%= get_admin_url(); %>/navs" class="list-group-item main  <% if(typeof site_info !== 'undefined' && (site_info.page_slug=='navs' || site_info.page_slug=='update_navs')) { %> active <% } %>">
                <i class="fa fa-bars" aria-hidden="true"></i>&nbsp; Navigations
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/navs" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='navs') { %> active <% } %>">
                All navigations
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/navs/create" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='create_navs') { %> active <% } %>">
                Add new
            </a>
        </li>
    </ul>
    <ul class="list-group">
        <li>
            <a href="<%= get_admin_url(); %>/users" class="list-group-item main <% if(typeof site_info !== 'undefined'  && (site_info.page_slug=='users' || site_info.page_slug=='update_user')) { %> active <% } %>">
                <i class="fa fa-users" aria-hidden="true"></i>&nbsp; Users
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/users" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='users') { %> active <% } %>">
                All users
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/users/create" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='create_user') { %> active <% } %>">
                Add new
            </a>
        </li>
        <li>
            <a href="<%= get_admin_url(); %>/users/profile" class="list-group-item <% if(typeof site_info !== 'undefined' && site_info.page_slug=='profile') { %> active <% } %>">
                Profile
            </a>
        </li>
    </ul>
    <% } else { %>
    <ul class="list-group">
        <li>
            <a href="<%= get_admin_url(); %>/users/profile" class="list-group-item main <% if(typeof site_info !== 'undefined' && site_info.page_slug=='profile') { %> active <% } %>">
                Profile
            </a>
        </li>
        </ul>
    <% } %>
    <ul class="list-group">
            <% if(administrator) { %>
                <li>
                        <a href="javascript:void(0)" class="list-group-item main">
                            <i class="fa fa-heart" aria-hidden="true"></i>&nbsp; Feedbacks
                            <span class="badge">79</span>
                        </a>
                    </li>
        <li>
            <a href="javascript:void(0)" class="list-group-item main">
                <i class="fa fa-cogs" aria-hidden="true"></i>&nbsp; Configs
            </a>
        </li>
       
        
        <% } %>
        <li>
            <a href="javascript:void(0)" class="list-group-item main" onClick="return confirm('Are you sure?')">
                <i class="fa fa-sign-out" aria-hidden="true"></i>&nbsp; Signout
            </a>
        </li>
    </ul>
</div>