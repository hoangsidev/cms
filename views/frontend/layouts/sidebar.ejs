<div class="col-sm-4">
            <h3>Categories</h3>
                <% if(typeof app_globals !== 'undefined' && typeof app_globals.categories !== 'undefined') { var categories = app_globals.categories; %>
                <ul>
                    <% if(categories.length > 0) { %>
                        <% categories.forEach(function(item, index){ %>
                             <li><a href="/category/<% if(item.slug) { %><%= item.slug %><% } %>"><% if(item.title) { %><%= item.title %><% } %></a></li>
                        <% }) %>
                    <% } %>
                    </ul>
                <% } %>
                <hr>

        <h3>Tags</h3>
                <% if(typeof app_globals !== 'undefined' && typeof app_globals.tags !== 'undefined') { var tags = app_globals.tags; %>
                <ul>
                    <% if(tags.length > 0) { %>
                        <% tags.forEach(function(item, index){ %>
                             <li><a href="/tag/<% if(item.slug) { %><%= item.slug %><% } %>"><% if(item.title) { %><%= item.title %><% } %></a></li>
                        <% }) %>
                    <% } %>
                    </ul>
                <% } %>
                <hr>



            </div>